{
  "name": "express-ajv-swagger-validation",
  "version": "0.11.2",
  "description": "Input validation using Swagger (Open API) and ajv",
  "main": "src/middleware.js",
  "types": "types/index.d.ts",
  "files": [
    "src",
    "types/index.d.ts"
  ],
  "nyc": {
    "report-dir": "./coverage",
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 90,
    "include": [
      "src/*.js"
    ],
    "exclude": [
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "lint:types": "dtslint types && eslint types/index.d.ts",
    "test": "nyc node_modules/mocha/bin/_mocha --recursive ./test/*/*/*-test.js ./test/*/*-test.js ./test/*-test.js && npm run lint:types",
    "node6-test": "nyc node_modules/mocha/bin/_mocha --recursive ./test/express/*/*-test.js ./test/express/*-test.js ./test/*-test.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "doctoc": "npm install -g doctoc && doctoc README.md",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Zooz/express-ajv-swagger-validation.git"
  },
  "keywords": [
    "ajv",
    "express",
    "swagger",
    "OpenAPI",
    "open api",
    "input validation",
    "validation",
    "validator",
    "koa"
  ],
  "bugs": {
    "url": "https://github.com/Zooz/express-ajv-swagger-validation"
  },
  "homepage": "https://github.com/Zooz/express-ajv-swagger-validation",
  "author": "Idan Tovi",
  "license": "Apache-2.0",
  "dependencies": {
    "api-schema-builder": "^1.0.9",
    "memoizee": "^0.4.14",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "ajv-keywords": "^3.4.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "clear-require": "^2.0.0",
    "coveralls": "^3.0.3",
    "dtslint": "^0.7.8",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.16.4",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.4.0",
    "mocha": "^6.1.3",
    "multer": "^1.4.1",
    "nyc": "^14.0.0",
    "request": "^2.88.0",
    "rewire": "^4.0.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "typescript": "3.4.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "snyk": true
}
