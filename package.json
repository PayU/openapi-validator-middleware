{
  "name": "express-ajv-swagger-validation",
  "version": "0.6.3",
  "description": "Input validation using Swagger (Open API) and ajv",
  "main": "index.js",
  "nyc": {
    "report-dir": "./coverage",
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 90,
    "include": [
      "src/*.js"
    ],
    "exclude": [
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "cache": true,
    "all": true
  },
  "scripts": {
    "test": "nyc node_modules/mocha/bin/_mocha --recursive ./test/*/*/*-test.js ./test/*/*-test.js ./test/*-test.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "doctoc": "npm install -g doctoc && doctoc README.md"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Zooz/express-ajv-swagger-validation.git"
  },
  "keywords": [
    "ajv",
    "express",
    "swagger",
    "OpenAPI",
    "open api",
    "input validation",
    "validation",
    "validator"
  ],
  "bugs": {
    "url": "https://github.com/Zooz/express-ajv-swagger-validation"
  },
  "homepage": "https://github.com/Zooz/express-ajv-swagger-validation",
  "author": "Idan Tovi",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.6.2",
    "lodash": "^4.17.11",
    "swagger-parser": "^6.0.2"
  },
  "devDependencies": {
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-sinon": "^2.8.1",
    "clear-require": "^2.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "mocha": "^4.0.1",
    "multer": "^1.3.0",
    "nyc": "^13.1.0",
    "request": "^2.88.0",
    "rewire": "^2.5.2",
    "sinon": "^4.0.1",
    "supertest": "^3.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
